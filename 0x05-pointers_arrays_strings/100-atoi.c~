#include "holberton.h"
/**
 *
 *
 *
 *
 */
int _atoi(char *s)
{
	char a, b;
	int i;

	while (*s != '\0')
	{
		if (*s >= '0' && *s <= '9')
		{
			a = a + *s;
		}
		else if ((*s == '+' && b == '+') || (*s == '-' && b == '-'))
		{
			b = '+';
		}
		else if ((*s == '-' && b == '+') || (*s == '+' && b '-'))
		{
			b = '-';
		}
	}
	s[0] = b;
	for (i = 1; a != '\0'; i++)
	{
		s[i] = a;
	}
}
